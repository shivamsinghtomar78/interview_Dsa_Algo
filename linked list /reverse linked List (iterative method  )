 // t.c=O(n)
// s.c=O(1)
class Solution {
    public ListNode reverseList(ListNode head) {
     ListNode curr=head;
     ListNode prev=null;
     ListNode Next=null;
    while(curr!=null){
        Next=curr.next;
        curr.next=prev;
        prev=curr;
        curr=Next;
    }  
    return prev; 
    }
}
